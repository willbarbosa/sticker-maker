{
  "name": "sticker-maker",
  "version": "0.1.0",
  "description": "A simple sticker maker",
  "scripts": {
    "dev": "next dev",
    "test": "concurrently --names next,jest --hide next --kill-others --success command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "build": "next build",
    "start": "next start",
    "lint:eslint:check": "next lint --dir src/",
    "lint:prettier:check": "prettier --check src/",
    "lint:prettier:fix": "prettier --write src/",
    "prepare": "husky",
    "commit": "cz"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "async-retry": "^1.3.3",
    "next": "14.2.26",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.26",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
